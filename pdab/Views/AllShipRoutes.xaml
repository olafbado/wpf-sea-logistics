<local:AllViewBase x:Class="pdab.Views.AllShipRoutes"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:pdab.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <DataGrid AutoGenerateColumns="False" ItemsSource="{Binding List}">
            <DataGrid.Columns>
                <DataGridTextColumn Header="RouteId" Binding="{Binding RouteId}"/>
                <DataGridTextColumn Header="ShipId" Binding="{Binding ShipId}"/>
                <DataGridTextColumn Header="DeparturePortId" Binding="{Binding DeparturePortId}"/>
                <DataGridTextColumn Header="ArrivalPortId" Binding="{Binding ArrivalPortId}"/>
                <DataGridTextColumn Header="DepartureDate" Binding="{Binding DepartureDate}"/>
                <DataGridTextColumn Header="ArrivalDate" Binding="{Binding ArrivalDate}"/>



            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</local:AllViewBase>
